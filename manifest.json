{

  "description": "Removes signature from sent mails in protonmail free account",
  "manifest_version": 2,
  "name": "ProSig",
  "version": "0.1",
  "homepage_url": "https://github.com/kskarthik/ProSig",
  "icons": {
    "32": "i32.png",
    "64": "i64.png"
  },

  "content_scripts": [
    {
      "matches": ["*://mail.protonmail.com/*"],
      "js": ["inject.js"]
    }
  ]
}